{
  "version": 2,
  "builds": [
    {
      "src": "**/*.php",
      "use": "vercel-php@0.7.4"
    },
    {
      "src": "*.css",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/chat.php", "dest": "/src/chat.php" },
    { "src": "/title.php", "dest": "/src/title.php" },
    { "src": "/", "dest": "/src/index.php" },
    { "src": "/output.css", "dest": "/src/output.css" },
    { "src": "/index.js", "dest": "/src/index.js" },
    { "src": "/title.js", "dest": "/src/title.js" },
    { "src": "/main.js", "dest": "/src/main.js" },
    { "src": "/index\\.php(/.*)?", "dest": "/src/index.php" }
  ],
   "buildCommand": "composer install --no-dev --optimize-autoloader && npm install"
}

